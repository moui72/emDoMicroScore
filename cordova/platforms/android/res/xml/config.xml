<?xml version='1.0' encoding='utf-8'?>
<widget id="us.peckenpaugh.emDoMsc" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <icon src="www/assets/images/icon.png" />
    <splash src="www/assets/images/emdoscf.png" />
    <name>EmDo Microcosm Scorer</name>
    <description>
        Scoresheet for EmDo Microcosm
    </description>
    <author email="tyler@ty-pe.com" href="http://ty-pe.com">
        Tyler J. Peckenpaugh
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
